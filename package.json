{
  "name": "vue-flex-touch",
  "version": "1.0.0",
  "description": "vue-flex-touch both for long press touch on mobile and right click on pc",
  "main": "dist/vue-flex-touch.common.js",
  "module": "dist/vue-flex-touch.esm.js",
  "scripts": {
